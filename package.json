    "react-select": "^5.2.1",
    "react-slideshow-image": "^3.7.0",
    "react-switch": "^6.0.0",
    "react-transition-group": "^4.4.2",
    "redux-thunk": "^2.4.0",
    "shortid": "^2.2.16",
    "sodium-native": "^3.3.0",
    "stellar-sdk": "^9.0.1",
