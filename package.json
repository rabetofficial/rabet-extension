{
  "name": "rabet",
  "version": "1.6.11",
  "description": "Rabet extension, a simple way to interact with Stellar network.",
  "main": "dist/electron.js",
  "scripts": {
    "electron": "electron .",
    "dev": "cross-env NODE_ENV=development PURPOSE=extension webpack --mode development",
    "build": "cross-env NODE_ENV=production PURPOSE=extension webpack --mode production",
    "build:desktop": "cross-env NODE_ENV=production PURPOSE=desktop webpack --mode production",
    "copy:build": "mkdir ../$BUILD; cp -r dist logo manifest.json ../$BUILD",
    "copy:source": "mkdir ../$BUILD; cp -r logo src .babelrc manifest.json package.json package-lock.json postcss.config.js webpack.config.js tsconfig.json externals.d.ts .eslintrc.json ../$BUILD",
    "package:linux": "electron-packager ./ rabet --platform=linux --asar --arch=x64 --icon=./desktop-logo/png/rabet256r.png --out=release --overwrite",
    "package:windows": "electron-packager . rabet --platform=win32 --asar --arch=x64 --icon=./desktop-logo/ico/rabet256t.ico --out=release --overwrite",
    "package:macos": "electron-packager ./ rabet --platform=darwin --asar --arch=x64 --icon=./desktop-logo/icns/rabet256r.icns --out=release --overwrite",
    "build:deb": "node packager/deb.js",
    "build:rpm": "node packager/redhat.js",
    "build:dmg": "electron-installer-dmg ./release/rabet-darwin-x64/rabet.app Rabet --icon=./desktop-logo/icns/rabet256r.icns --out=./release/installers/ --title=Rabet --overwrite",
    "build:windows": "node packager/win.js"
  },
  "repository": {
    "type": "git",
    "url": "git@github.com:rabetofficial/rabet-extension.git"
  },
  "keywords": [
    "stellar",
    "wallet",
    "extension"
  ],
  "author": "Alper Polat <alper95polat@gmail.com>",
  "license": "ISC",
  "devDependencies": {
    "@babel/core": "^7.16.0",
    "@babel/preset-env": "^7.16.0",
    "@babel/preset-react": "^7.16.0",
    "@types/luxon": "^2.0.9",
    "@types/qrcode.react": "^1.0.2",
    "@types/react-copy-to-clipboard": "^5.0.2",
    "@types/react-modal": "^3.13.1",
    "@types/react-redux": "^7.1.22",
    "@types/shortid": "0.0.29",
    "@types/styled-components": "^5.1.20",
    "@typescript-eslint/eslint-plugin": "^5.3.0",
    "@typescript-eslint/parser": "^5.3.0",
    "autoprefixer": "^10.4.2",
    "babel-loader": "^8.2.3",
    "cross-env": "^7.0.3",
    "crypto-browserify": "^3.12.0",
    "css-loader": "^6.5.1",
    "css-minimizer-webpack-plugin": "^3.1.1",
    "electron-installer-debian": "^3.1.0",
    "electron-installer-redhat": "^3.3.0",
    "eslint": "^7.32.0",
    "eslint-config-airbnb": "^18.2.1",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-import": "^2.25.2",
    "eslint-plugin-jsx-a11y": "^6.4.1",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-react": "^7.26.1",
    "eslint-plugin-react-hooks": "^4.2.0",
    "file-loader": "^6.2.0",
    "html-minimizer-webpack-plugin": "^3.3.0",
    "html-webpack-plugin": "^5.5.0",
